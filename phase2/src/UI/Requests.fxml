<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.RequestController">
  <center>
    <AnchorPane BorderPane.alignment="CENTER">
      <children>
            <VBox layoutX="238.0" layoutY="161.0" prefHeight="79.0" prefWidth="125.0">
               <children>
              <Button fx:id="newRequestButton" mnemonicParsing="false" onAction="#newRequestButtonOnAction" text="Create new Request">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
              <Button fx:id="viewRequestButton" mnemonicParsing="false" onAction="#viewRequestButtonOnAction" text="View your Requests">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Go Back" />
      </children>
    </AnchorPane>
  </center>
</BorderPane>
