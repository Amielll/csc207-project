      <children>
            <Button fx:id="goBackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Go Back" onAction="#goBackButtonOnAction" />
            <Button fx:id="newRequestButton" layoutX="387.0" layoutY="355.0" mnemonicParsing="false" onAction="#newRequestButtonOnAction" prefHeight="25.0" prefWidth="160.0" text="Create New Request" />
            <TableView layoutX="44.0" layoutY="39.0" prefHeight="311.0" prefWidth="499.0">
              <columns>
                <TableColumn prefWidth="425.0" text="Your Request" />
                  <TableColumn prefWidth="75.0" text="Status" />
              </columns>
            </TableView>
      </children>
    </AnchorPane>
  </center>
</BorderPane>
