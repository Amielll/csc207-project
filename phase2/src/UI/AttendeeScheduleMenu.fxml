<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.AttendeeScheduleMenuController">
   <children>
      <Button fx:id="goBackButton" layoutY="1.0" mnemonicParsing="false" onAction="#goBackButtonOnAction" text="Go Back" />
      <DatePicker fx:id="datePicker" editable="false" layoutX="15.0" layoutY="40.0" showWeekNumbers="true" />
      <ComboBox fx:id="roomFilterComboBox" layoutX="225.0" layoutY="40.0" prefWidth="150.0" promptText="Pick a Room" />
      <ComboBox fx:id="eventTypeFilterComboBox" layoutX="398.0" layoutY="40.0" prefWidth="150.0" promptText="Pick an Event Type" />
   </children>
</AnchorPane>
