<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="250.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.OrganizerCancelEventController">
    <center>
        <AnchorPane BorderPane.alignment="CENTER">
            <children>
                <TextField fx:id="eventNameTextField" layoutX="196.0" layoutY="77.0" promptText="Event Name" />
                <Label layoutX="90.0" layoutY="77.0" text="Event Name">
                    <font>
                        <Font size="19.0" />
                    </font>
                </Label>

                <Button fx:id="cancelEventButton" layoutX="232.0" layoutY="150.0" mnemonicParsing="false" onAction="#cancelEventButtonOnAction" text="Cancel" />
                <Label fx:id="eventNameErrorLabel" layoutX="196.0" layoutY="130.0" />
                <Button fx:id="goBackButton" layoutX="6.0" layoutY="2.0" mnemonicParsing="false" onAction="#goBackButtonOnAction" text="Go Back" />
                <Label fx:id="cancelEventSuccessLabel" layoutX="206.0" layoutY="130.0" />
            </children>
        </AnchorPane>
    </center>

</BorderPane>
