<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns="http://javafx.com/javafx"
            prefHeight="400.0" prefWidth="600.0"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="UI.Request.RequestController">
  <center>
    <AnchorPane BorderPane.alignment="CENTER">
      <children>
        <Button fx:id="newRequestButton" layoutX="12.0" layoutY="11.0" mnemonicParsing="false" onAction="#newRequestButtonOnAction" text="Create new Request"/>
        <Button fx:id="viewRequestButton" layoutX="12.0" layoutY="20.0" mnemonicParsing="false" onAction="#viewRequestButtonOnAction" text="View your Requests"/>
      </children>
    </AnchorPane>
  </center>
</BorderPane>
